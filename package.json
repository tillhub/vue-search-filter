{
  "name": "@tillhub/vue-search-filter",
  "version": "0.0.3",
  "description": "Search input field widht dropdown for filters",
  "author": "Quynh To Tuan <quynh.totuan@gmail.com>",
  "license": "MIT",
  "repository": "tillhub/vue-search-filter",
  "private": false,
  "main": "./dist/vue-search-filter.common.js",
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js",
    "build:lib": "vue-cli-service build --target lib --name vue-search-filter src/index.vue",
    "build": "npm run clean && npm run build:lib",
    "clean": "rimraf dist",
    "lint": "vue-cli-service lint",
    "semantic-release": "semantic-release",
    "precommit": "git add .",
    "commit": "npm run precommit && git-cz"
  },
  "dependencies": {
    "@tillhub/vue-date-picker": "^1.7.0",
    "date-fns": "^1.29.0",
    "typeface-lato": "0.0.54",
    "v-calendar": "^0.9.7",
    "vue-i18n": "^8.2.1"
  },
  "peerDependencies": {
    "vue": "^2.5.2",
    "element-ui": "^2.3.6"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "@semantic-release/github": "^5.0.6",
    "@semantic-release/npm": "^5.0.4",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^3.0.5",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "element-ui": "^2.3.6",
    "gh-pages": "^1.2.0",
    "rimraf": "^2.6.0",
    "semantic-release": "^15.9.17",
    "validate-commit-msg": "^2.14.0",
    "vue": "^2.5.2",
    "vue-template-compiler": "^2.5.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/npm",
      "@semantic-release/changelog",
      {
        "path": "@semantic-release/git",
        "assets": [
          "dist",
          "package.json",
          "package-lock.json",
          "src",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
